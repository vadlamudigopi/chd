        if (check == "No") {
            $("#ResidentsLockedError").fadeIn();
            $("#ResidentsLockedWODC").attr('required', true);
            $("#ResidentsLockedError").html("<center style='color:red'>Please select required field.<center>").fadeOut(6000);
        } else {
            $("#ResidentsLockedWODC").attr('required', false);
        }
    }
    </script>*@