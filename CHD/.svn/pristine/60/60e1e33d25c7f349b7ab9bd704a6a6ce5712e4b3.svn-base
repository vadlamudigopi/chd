<form class="form-sample" id="HumanRightsForm" method="POST"  enctype="multipart/form-data">
    <section id="HumanRights" class="SurveySection">
        <h4>Human Rights</h4>
        <div class="col-md-12">
            <div class="form-group row">
                <label class="col-sm-6 col-form-label">Mandated Reporter numbers are posted by phonet: </label>
                <div class="col-sm-3" style="display: flex;">
                    <div class="form-radio" style="margin-top: auto;">
                        <label class="form-check-label">
                            <input type="radio" class="form-check-input" name="HumanMandated" id="HumanMandatedYes" value="Yes" data-validation-error-msg-container="#HumanMandatedError" data-validation-error-msg="Please select Mandated Reporter numbers are posted by phonet" data-validation="required">
                            Yes
                        </label>
                    </div>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <div class="form-radio" style="margin-top: auto;">
                        <label class="form-check-label">
                            <input type="radio" class="form-check-input" name="HumanMandated" id="HumanMandatedNo" data-validation-error-msg-container="#HumanMandatedError" data-validation-error-msg="Please select Mandated Reporter numbers are posted by phonet" value="No">
                            No
                        </label>
                    </div>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <div class="form-radio" style="margin-top: auto;">
                        <label class="form-check-label">
                            <input type="radio" class="form-check-input" name="HumanMandated" id="HumanMandatedNA" data-validation-error-msg-container="#HumanMandatedError" data-validation-error-msg="Please select Mandated Reporter numbers are posted by phonet" value="NA">
                            N/A
                        </label>
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="col-md-12">
                        <div class="row">
                            <input type="text" placeholder="Work Order Date/Comments" name="HumanMandatedWODC" id="HumanMandatedWODC" data-validation-error-msg-container="#HumanMandatedWODCError" data-validation-error-msg="Enter Work Order Date/Comments" class="form-control" />
                            <span id="HumanMandatedWODCError"></span>
                        </div>
                    </div>
                </div>
                <div class="col-sm-5" id="HumanMandatedError"></div>
            </div>
        </div>
        <hr />



        <div class="col-md-12">
            <div class="form-group row">
                <label class="col-sm-6 col-form-label">Staff knows about mandated reporting procedures.: </label>
                <div class="col-sm-3" style="display: flex;">
                    <div class="form-radio" style="margin-top: auto;">
                        <label class="form-check-label">
                            <input type="radio" class="form-check-input" name="HumanStaff" id="HumanStaffYes" value="Yes" data-validation-error-msg-container="#HumanStaffError" data-validation-error-msg="Please select Staff knows about mandated reporting procedures." data-validation="required">
                            Yes
                        </label>
                    </div>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <div class="form-radio" style="margin-top: auto;">
                        <label class="form-check-label">
                            <input type="radio" class="form-check-input" name="HumanStaff" id="HumanStaffNo" data-validation-error-msg-container="#HumanStaffError" data-validation-error-msg="Please select Staff knows about mandated reporting procedures." value="No">
                            No
                        </label>
                    </div>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <div class="form-radio" style="margin-top: auto;">
                        <label class="form-check-label">
                            <input type="radio" class="form-check-input" name="HumanStaff" id="HumanStaffNA" data-validation-error-msg-container="#HumanStaffError" data-validation-error-msg="Please select Staff knows about mandated reporting procedures." value="NA">
                            N/A
                        </label>
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="col-md-12">
                        <div class="row">
                            <input type="text" placeholder="Work Order Date/Comments" name="HumanStaffWODC" id="HumanStaffWODC" data-validation-error-msg-container="#HumanStaffWODCError" data-validation-error-msg="Enter Work Order Date/Comments" class="form-control" />
                            <span id="HumanStaffWODCError"></span>
                        </div>
                    </div>
                </div>
                <div class="col-sm-5" id="HumanStaffError"></div>
            </div>
        </div>
        <hr />





        <div class="col-md-12">
            <div class="form-group row">
                <label class="col-sm-6 col-form-label">HIPAA Privacy compliance posted: </label>
                <div class="col-sm-3" style="display: flex;">
                    <div class="form-radio" style="margin-top: auto;">
                        <label class="form-check-label">
                            <input type="radio" class="form-check-input" name="HumanHIPAA" id="HumanHIPAAYes" value="Yes" data-validation-error-msg-container="#HumanHIPAAError" data-validation-error-msg="Please select HIPAA Privacy compliance posted" data-validation="required">
                            Yes
                        </label>
                    </div>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <div class="form-radio" style="margin-top: auto;">
                        <label class="form-check-label">
                            <input type="radio" class="form-check-input" name="HumanHIPAA" id="HumanHIPAANo" data-validation-error-msg-container="#HumanHIPAAError" data-validation-error-msg="Please select HIPAA Privacy compliance posted" value="No">
                            No
                        </label>
                    </div>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    <div class="form-radio" style="margin-top: auto;">
                        <label class="form-check-label">
                            <input type="radio" class="form-check-input" name="HumanHIPAA" id="HumanHIPAANA" data-validation-error-msg-container="#HumanHIPAAError" data-validation-error-msg="Please select HIPAA Privacy compliance posted" value="NA">
                            N/A
                        </label>
                    </div>
                </div>
                <div class="col-sm-3">
                    <div class="col-md-12">
                        <div class="row">
                            <input type="text" placeholder="Work Order Date/Comments" name="HumanHIPAAWODC" id="HumanHIPAAWODC" data-validation-error-msg-container="#HumanHIPAAWODCError" data-validation-error-msg="Enter Work Order Date/Comments" class="form-control" />
                            <span id="HumanHIPAAWODCError"></span>
                        </div>
                    </div>
                </div>
                <div class="col-sm-5" id="HumanHIPAAError"></div>
            </div>
        </div>




        <div class="col-md-12">
            <button type="button" class="btn btn-success btn-fw previous" id="HumanRightsPrevious" onclick="goPreviousForm('OTHERITEMS', 'OTHERITEMSNext', 'OTHERITEMSPrevious');">Previous</button>
            <button type="submit" class="btn btn-success btn-fw" id="HumanRightsNext">Next</button>
        </div>
    </section>
</form>

